@page
@model UnikOnBoarding.Pages.Booking.CreateModel
@{
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"></script>
<link rel="stylesheet" href="~/css/homemadestyle.css" asp-append-version="true" />

<h2>Book en Ny Tid</h2>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                @*<label asp-for="ProjectWithUserModel.ProjectCreateModel.ProjectName"></label>*@
                Vælg Dato: <input asp-for="DateTime" asp-format="{0:yyyy-MM-ddTHH:mm}" class="form-control" />
                @*<input asp-for="ProjectWithUserModel.ProjectCreateModel.ProjectName" class="form-control" />*@
                <span asp-validation-for="DateTime" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-success">Opret Booking</button>
        </form>
    </div>
</div>